{
  "name": "jx-response-generator",
  "version": "1.0.8",
  "description": "A simple library that helps developers to make quick api responses from multiple responses.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "Ayush papnai",
  "keywords": [
    "response changer",
    "Api",
    "formatting",
    "react",
    "axios",
    "javascript",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pappijx/npm_package.git"
  },
  "license": "MIT",
  "devDependencies": {
    "prettier": "2.8.6",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "5.0.2"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ]
}
